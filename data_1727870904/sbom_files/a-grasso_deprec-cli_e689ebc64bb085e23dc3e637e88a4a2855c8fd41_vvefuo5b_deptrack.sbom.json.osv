            {
              "modified": "2024-03-29T15:57:07Z",
              "published": "2022-11-23T22:17:25Z",
              "schema_version": "1.6.0",
              "id": "GHSA-562r-vg33-8x8h",
              "aliases": [
                "BIT-postgresql-jdbc-driver-2022-41946",
