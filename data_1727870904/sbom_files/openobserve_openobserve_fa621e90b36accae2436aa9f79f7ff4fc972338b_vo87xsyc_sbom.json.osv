          "vulnerabilities": [
            {
              "modified": "2024-09-26T21:11:53Z",
              "published": "2024-09-23T22:11:02Z",
              "schema_version": "1.6.0",
              "id": "GHSA-gcx4-mw62-g8wm",
              "aliases": [
